<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/css/bootstrap-select.min.css">
    <style>
        body {
            background-color: transparent;
        }

        .profile-form {
            min-height: 500px;
        }

        .navbar-custom {
            background-color: rgb(67,101,226);
        }

        .navbar-text {
            color: white;
        }
    </style>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand navbar-custom">
            <img href="co-researcher" src="/images/uniLogo.png" alt="University of Dundee" />
            <div class="collapse navbar-collapse" .id="navbarAltMarkup">
                <div class="navbar-nav ml-5">
                    <a class="nav-item nav-link ml-4 mr-4 navbar-text " asp-area="" asp-page="/ExperimentManager">Experiment Manager</a>
                    <a class="nav-item nav-link ml-4 mr-4 navbar-text " asp-area="" asp-page="/ExperimentDashboard">Experiment Dashboard</a>
                    <a class="nav-item nav-link ml-4 mr-4 navbar-text " asp-area="" asp-page="/questionnaireManagment">Questionnaire Dashboard</a>
                </div>
            </div>
        </nav>
    </header>


    @RenderBody();

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - CourseworkAC31007Agile - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/js/bootstrap-select.min.js"></script>
</body>
</html>
